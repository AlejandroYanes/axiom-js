{
    "name": "@axiomhq/kit",
    "description": "The official javascript kit for the Axiom API",
    "version": "0.13.0",
    "author": "Axiom, Inc.",
    "license": "MIT",
    "contributors": [
        "Islam Shehata <islam@axiom.co>"
    ],
    "engines": {
        "node": ">=14"
    },
    "main": "dist/cjs/index.js",
    "types": "dist/types/index.d.ts",
    "type": "module",
    "scripts": {
        "build": "tsc -b",
        "build:cjs": "tsc --project tsconfig.cjs.json",
        "format": "eslint '*/**/*.{js,ts}' --quiet --fix",
        "lint": "eslint '*/**/*.{js,ts}'",
        "prepublish": "npm run build && npm run build:cjs",
        "test": "jest test/unit",
        "cover": "nyc -r text -e .ts -x 'tests/unit/**/*.test.ts' npm run test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/axiomhq/axiom-js.git"
    },
    "keywords": [
        "axiom",
        "logger",
        "kit"
    ],
    "bugs": {
        "url": "https://github.com/axiomhq/axiom-js/issues"
    },
    "homepage": "https://github.com/axiomhq/axiom-js",
    "dependencies": {
        "cross-fetch": "^3.1.5",
        "fetch-retry": "^5.0.3",
        "uuid": "^8.3.2"
    },
    "exports": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js",
        "default": "./dist/esm/index.js"
    }
}
