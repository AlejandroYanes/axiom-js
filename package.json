{
    "name": "axiom-node",
    "description": "TypeScript bindings for the Axiom API.",
    "version": "1.0.0",
    "author": "Axiom, Inc.",
    "license": "MIT",
    "contributors": [
        "Lukas Malkmus <mail@lukasmalkmus.com> (https://github.com/lukasmalkmus)"
    ],
    "engines": {
        "node": ">=10"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "lint": "eslint '*/**/*.{js,ts}' --quiet --fix",
        "prepublish": "yarn build",
        "test": "mocha -r ts-node/register tests/**/*.test.ts",
        "cover": "nyc -r text -e .ts -x 'tests/*.test.ts' yarn test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/axiomhq/axiom-node.git"
    },
    "keywords": [
        "axiom",
        "api",
        "rest",
        "client"
    ],
    "bugs": {
        "url": "https://github.com/axiomhq/axiom-node/issues"
    },
    "homepage": "https://github.com/axiomhq/axiom-node",
    "dependencies": {
        "axios": "^0.21.1"
    },
    "devDependencies": {
        "@types/chai": "^4.2.15",
        "@types/mocha": "^8.2.1",
        "@typescript-eslint/eslint-plugin": "^4.14.0",
        "@typescript-eslint/parser": "^4.14.0",
        "chai": "^4.3.0",
        "eslint": "^7.18.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-prettier": "^3.3.1",
        "mocha": "^8.3.0",
        "nyc": "^15.1.0",
        "prettier": "^2.2.1",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.2"
    }
}